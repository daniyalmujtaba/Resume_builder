{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],

    "rules": { 
        "no-console": 1,
        "semi":"warn",
        "prettier/prettier":"warn",
        "eqeqeq":"warn"


      }
    
  }